1

A = input().split(',')
B = input().split(',')
C = []
for i in A:
    if i.find(str(B)):
        C.append(i)
print(C)

2

a = input().split('.')
x = int(a[0])
y = int(a[1])
z = int(a[2])
if z < 9:
    z += 1
else:
    z = 0
    y += 1
if y > 9:
    y = 0
    x += 1

print(f'{x}.{y}.{z}')

3

a = input().split()
for i in a:
    if len(i) % 2 == 0:
        print(i)

