// 26 var-8

import java.util.Scanner;

public class oop.lab_8.Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.println("Введи число а");
        double a = scanner.nextInt();
        System.out.println("Введи число b");
        double b = scanner.nextInt();
        double N = 26;
        double answer;
        answer = Math.abs(((a - N) /(2*b))+((a * a + b * b * b) /N));
        System.out.println("Ответ: ");
        System.out.printf("%.2f", answer);

    }
}
import java.util.Scanner;

public class oop.lab_8.Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.println("Input text");
        String text = scanner.nextLine();
        System.out.println("Position(index start with 0)");
        int position = scanner.nextInt();
        while (position < 0) {
            System.out.println("number < 0");
            position = scanner.nextInt();
        }
        System.out.println("Input symbol");
        char ch = scanner.next().charAt(0);
        text = text.substring(0, position) + ch + text.substring(position + 1);
        System.out.println(text.toUpperCase());
    }
}
    }
}

import java.util.Scanner;
public class oop.lab_8.Main {
    public static void main(String[] args) {
        int N = 26;
        double cost = 123.45 * N;
        double discount =0.95;
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        double result = cost * n;
        result = result * discount;
        System.out.println(result);
        System.out.printf("%.2f", result);
        }
}

import java.util.Scanner;
public class oop.lab_8.Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        char symbol = scanner.next().charAt(0);
        int n = 26;
        int t = symbol + n;
        char a = (char) t;
        System.out.println(a);
    }
}
